{
    "name": "yelpcamp",
    "version": "1.0.0",
    "private": true,
    "main": "app.js",
    "scripts": {
        "lint": "eslint --fix --max-warnings=0 .",
        "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky && sort-package-json; fi",
        "prettier": "prettier --write .",
        "seed": "dotenvx run -f .env.dev -- node seeds/index.js",
        "seed:prod": "dotenvx run -f .env.prod -- node seeds/index.js",
        "start": "dotenvx run -f .env.dev -- nodemon -e js,ejs,json app.js",
        "start:deploy": "dotenvx run -- node app.js"
    },
    "lint-staged": {
        "*": [
            "eslint --fix --max-warnings=0 --no-warn-ignored",
            "prettier --write --ignore-unknown"
        ]
    },
    "dependencies": {
        "@dotenvx/dotenvx": "^1.51.4",
        "axios": "^1.13.2",
        "cloudinary": "^1.41.3",
        "connect-flash": "^0.1.1",
        "connect-mongo": "^6.0.0",
        "ejs-mate": "^4.0.0",
        "express": "^5.2.1",
        "express-mongo-sanitize": "^2.2.0",
        "express-session": "^1.18.2",
        "helmet": "^8.1.0",
        "joi": "^18.0.2",
        "lorem-ipsum": "^2.0.8",
        "method-override": "^3.0.0",
        "mongoose": "^9.1.2",
        "multer": "^2.0.2",
        "multer-storage-cloudinary": "^4.0.0",
        "nodemon": "^3.1.11",
        "passport": "^0.7.0",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^9.0.1",
        "sanitize-html": "^2.17.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "depcheck": "^1.4.7",
        "eslint": "^9.39.2",
        "eslint-plugin-import": "^2.32.0",
        "globals": "^17.0.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "npm-check-updates": "^19.3.1",
        "prettier": "^3.7.4",
        "sort-package-json": "^3.6.0"
    }
}
